{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "extends": [
    "airbnb"
  ],
  "globals": {
    "window": true,
    "navigator": true,
    "fetch": true,
    "document": true,
    "it": true,
    "Feature": true,
    "Scenario": true,
    "actor": true,
    "expect": true,
    "FormData": true,
    "cy": true,
    "describe": true,
    "Cypress": true,
    "beforeEach": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": [
          "./src",
          "src"
        ]
      }
    }
  },
  "rules": {
    "react/jsx-props-no-spreading": "off",
    "import/no-unresolved": "off",
    "import/no-extraneous-dependencies": "off",
    "no-underscore-dangle": 0,
    "import/prefer-default-export": 0,
    "import/no-dynamic-require": 0,
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 2,
        "maxEOF": 0
      }
    ],
    "global-require": 0,
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "quotes": [
      2,
      "single",
      {
        "avoidEscape": true
      }
    ]
  },
  "overrides": []
}